{
  "name": "stef",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production bankai start index.js",
    "dev": "NODE_ENV=development bankai start index.js",
    "build": "bankai build index.js",
    "build-icons": "svgo -f assets",
    "inspect": "bankai inspect index.js",
    "test": "standard && npm run test-deps",
    "test-deps": "dependency-check . -i sheetify && dependency-check . --extra --no-dev -i sanitize.css",
    "publish": "git push && npm run build && surge -p ./dist -d stef.surge.sh"
  },
  "dependencies": {
    "choo": "^6.0.0-4",
    "choo-devtools": "^1.3.1",
    "choo-log": "^7.2.1",
    "choo-service-worker": "^2.1.6",
    "sanitize.css": "^5.0.0"
  },
  "devDependencies": {
    "bankai": "^9.0.0-1",
    "standard": "^10.0.2",
    "svgo": "^0.7.2"
  }
}
